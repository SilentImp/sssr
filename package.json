    "dotenv": "^5.0.0",
    "eslint-plugin-react": "^7.5.1",
    "express": "^4.16.2",
    "express-cache-headers": "^0.1.3",
    "history": "^4.7.2",
    "husky": "^0.14.3",
    "i18next": "^10.6.0",
    "i18next-browser-languagedetector": "^2.1.0",
